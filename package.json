    "nodemailer": "^7.0.6",
    "pg": "^8.15.5",
    "prisma-extension-pagination": "^0.7.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.2",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-toastify": "^11.0.5",
    "react-top-loading-bar": "^3.0.2",
    "server-only": "^0.0.1",
    "zod": "^4.1.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar": "^4.0.3",

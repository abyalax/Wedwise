{
  "name": "boilerplate-nodejs-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "permissions:generate": "env-cmd -f .env tsx scripts/generate-route-permissions.ts",
    "permissions:watch": "env-cmd -f .env tsx scripts/generate-route-permissions.ts --watch",
    "dev": "concurrently \"pnpm run permissions:watch\" \"next dev --turbopack\"",
    "build": "pnpm permissions:generate && next build --turbopack",
    "test": "env-cmd -f .env vitest run --config ./vitest.config.ts",
    "start": "next start",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "typecheck": "tsc -b --noEmit",
    "db:seed": "env-cmd -f .env npx tsx db/seed.ts",
    "db:delete": "env-cmd -f .env npx tsx db/delete.ts",
    "db:generate": "env-cmd -f .env prisma generate",
    "db:migrate": "env-cmd -f .env prisma migrate dev",
    "db:migrate:deploy": "env-cmd -f .env prisma migrate deploy",
    "db:studio": "env-cmd -f .env prisma studio",
    "db:push": "env-cmd -f .env prisma db push",
    "db:pull": "env-cmd -f .env prisma db pull",
    "db:query": "env-cmd -f .env tsx db/query.ts"
  },
  "dependencies": {
    "@google/genai": "^1.21.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.90.1",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.0",
    "chartjs-chart-matrix": "^3.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.19",
    "immer": "^10.1.3",
    "jsonwebtoken": "^9.0.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
